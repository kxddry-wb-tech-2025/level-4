{
  "uid": "go-runtime",
  "title": "Go Runtime",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m"] },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Scheduler",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "go_sched_gomaxprocs_threads",
          "legendFormat": "GOMAXPROCS"
        },
        {
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "go_threads",
          "legendFormat": "OS Threads"
        },
        {
          "refId": "C",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "go_goroutines",
          "legendFormat": "Goroutines"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Garbage Collector (pause quantiles)",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 9 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "go_gc_duration_seconds",
          "legendFormat": "q={{quantile}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "tooltip": { "mode": "multi" }
      }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [ { "type": "dashboard", "name": "Annotations & Alerts", "enable": true } ] }
}


